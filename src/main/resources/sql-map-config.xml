<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC 
  "-//mybatis.org//DTD Config 3.0//EN" 
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
  <typeAliases>
    <typeAlias alias="ProductVO"  type="com.eighty.product.ProductVO" />
    <typeAlias alias="UsersVO"  type="com.eighty.users.UsersVO" />
    <typeAlias alias="PurchaseVO"  type="com.eighty.purchase.PurchaseVO" />
    <typeAlias alias="ReviewVO"  type="com.eighty.review.ReviewVO" />
  </typeAliases>

  <mappers>  
    <mapper resource="mappings/product-mapping.xml"  />
    <mapper resource="mappings/users-mapping.xml"  />
    <mapper resource="mappings/review-mapping.xml"  />
    <mapper resource="mappings/purchase-mapping.xml"  />
  </mappers>
  
</configuration>